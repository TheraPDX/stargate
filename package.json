{
  "name": "stargate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:js && node web/src/server.js",
    "startDev": "npm run build:js && node web/src/server.js",
    "live": "npm run watch:js & npm run watch:css & nodemon web/src/server.js",
    "build:js": "browserify web/src/app.jsx -t babelify --outfile web/assets/app.js && uglifyjs web/assets/app.js -o web/assets/app.min.js",
    "watch:js": "watchify ./web/src/app.jsx -t babelify --outfile ./web/assets/app.js",
    "watch:css": "stylus -w --include-css --compress ./web/src/style/app.styl -o ./web/assets/",
    "build:css": "stylus --include-css --compress ./web/src/style/app.styl -o ./web/assets/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.12.0",
    "babel": "^4.7.16",
    "babelify": "^5.0.4",
    "body-parser": "^1.15.2",
    "browserify": "^9.0.3",
    "connect-cachify": "0.0.17",
    "ejs": "^2.3.1",
    "express": "^4.12.3",
    "icono": "^1.3.0",
    "isomorphic-fetch": "^2.0.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-router": "^2.5.2",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "nodemon": "^1.9.2",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  }
}
